extends layouts/main
include mixins/videoBlock

block content
	.search__header
		h3 Searching : #{searchingBy}
	.search__videos
		if videos.length === 0
			h5 NO Videos Found ㅜㅜ
		each item in videos
			+videoBlock({
				title:item.title,
				views:item.views,
				videoFile:item.videoFile,
				id:item.id
			})
